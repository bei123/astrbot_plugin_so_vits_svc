{
    "base_setting": {
        "description": "基础设置",
        "type": "object",
        "hint": "API服务器的基本配置",
        "items": {
            "base_url": {
                "description": "API服务器地址",
                "type": "string",
                "hint": "如果是本地部署，可以使用 http://127.0.0.1:1145",
                "default": "default"
            },
            "timeout": {
                "description": "请求超时时间(秒)",
                "type": "int",
                "hint": "转换请求的超时时间",
                "default": 300
            },
            "msst_url": {
                "description": "MSST-WebUI API地址",
                "type": "string",
                "hint": "MSST-WebUI 的 API 地址",
                "default": "default"
            },
            "msst_preset": {
                "description": "MSST预设文件路径",
                "type": "string",
                "hint": "MSST 处理使用的预设文件路径",
                "default": "wav.json"
            },
            "model_dir": {
                "description": "默认模型目录",
                "type": "string",
                "hint": "默认使用的模型目录名称，需要在model目录下存在对应的模型文件",
                "default": "default"
            },
            "netease_cookie": {
                "description": "网易云音乐Cookie",
                "type": "string",
                "hint": "用于访问网易云音乐API的Cookie",
                "default": "default"
            },
            "bbdown_cookie": {
                "description": "哔哩哔哩Cookie",
                "type": "string",
                "hint": "用于访问哔哩哔哩API的Cookie，格式为SESSDATA=xxx;bili_jct=xxx;DedeUserID=xxx",
                "default": "default"
            },
            "douyin_cookie": {
                "description": "抖音Cookie",
                "type": "string",
                "hint": "用于访问抖音API的Cookie，提高下载成功率。请从浏览器开发者工具中复制抖音网站的Cookie",
                "default": "default"
            }
        }
    },
    "voice_config": {
        "description": "语音转换设置",
        "type": "object",
        "hint": "语音转换的相关参数设置",
        "items": {
            "enable_mixing": {
                "description": "是否开启混音",
                "type": "bool",
                "hint": "是否将转换后的人声与原伴奏混音",
                "default": true
            },
            "mixing_params": {
                "description": "混音参数设置",
                "type": "object",
                "hint": "混音处理的相关参数设置",
                "items": {
                    "sample_rate": {
                        "description": "采样率",
                        "type": "int",
                        "hint": "音频处理的采样率",
                        "default": 44100
                    },
                    "headroom": {
                        "description": "伴奏增益",
                        "type": "float",
                        "hint": "伴奏的音量增益，单位dB",
                        "default": -8
                    },
                    "voc_input": {
                        "description": "人声输入增益",
                        "type": "float",
                        "hint": "人声的输入增益，单位dB",
                        "default": -4
                    },
                    "revb_gain": {
                        "description": "混响增益",
                        "type": "float",
                        "hint": "混响效果的增益，单位dB",
                        "default": 0
                    },
                    "comp_release": {
                        "description": "压缩器释放时间",
                        "type": "int",
                        "hint": "压缩器的释放时间(ms)",
                        "default": 300
                    },
                    "comp_feedback": {
                        "description": "压缩器反馈时间",
                        "type": "int",
                        "hint": "压缩器的反馈时间(ms)",
                        "default": 180
                    },
                    "reverb_short": {
                        "description": "短混响时间",
                        "type": "int",
                        "hint": "短混响的时间(ms)",
                        "default": 5
                    },
                    "reverb_medium": {
                        "description": "中混响时间",
                        "type": "int",
                        "hint": "中混响的时间(ms)",
                        "default": 25
                    },
                    "reverb_long": {
                        "description": "长混响时间",
                        "type": "int",
                        "hint": "长混响的时间(ms)",
                        "default": 50
                    },
                    "delay_time": {
                        "description": "延迟时间",
                        "type": "int",
                        "hint": "延迟效果的时间(ms)",
                        "default": 200
                    }
                }
            },
            "max_queue_size": {
                "description": "最大队列大小",
                "type": "int",
                "hint": "超过此队列大小将拒绝新的转换请求",
                "default": 100
            },
            "default_speaker": {
                "description": "默认说话人ID",
                "type": "string",
                "hint": "默认使用的说话人ID",
                "default": "0"
            },
            "default_pitch": {
                "description": "默认音调调整",
                "type": "int",
                "hint": "默认的音调调整值，范围-12到12",
                "default": 0
            },
            "default_k_step": {
                "description": "默认扩散步数",
                "type": "int",
                "hint": "默认的扩散步数",
                "default": 100
            },
            "default_shallow_diffusion": {
                "description": "默认使用浅扩散",
                "type": "bool",
                "hint": "是否默认使用浅扩散",
                "default": true
            },
            "default_only_diffusion": {
                "description": "默认使用纯扩散",
                "type": "bool",
                "hint": "是否默认使用纯扩散",
                "default": false
            },
            "default_cluster_infer_ratio": {
                "description": "默认聚类推理比例",
                "type": "float",
                "hint": "默认的聚类推理比例",
                "default": 0
            },
            "default_auto_predict_f0": {
                "description": "默认自动预测音高",
                "type": "bool",
                "hint": "是否默认自动预测音高",
                "default": false
            },
            "default_noice_scale": {
                "description": "默认噪声比例",
                "type": "float",
                "hint": "默认的噪声比例",
                "default": 0.4
            },
            "default_f0_filter": {
                "description": "默认过滤F0",
                "type": "bool",
                "hint": "是否默认过滤F0",
                "default": false
            },
            "default_f0_predictor": {
                "description": "默认F0预测器",
                "type": "string",
                "hint": "默认使用的F0预测器",
                "default": "fcpe"
            },
            "default_enhancer_adaptive_key": {
                "description": "默认增强器自适应键",
                "type": "int",
                "hint": "默认的增强器自适应键值",
                "default": 0
            },
            "default_cr_threshold": {
                "description": "默认交叉参考阈值",
                "type": "float",
                "hint": "默认的交叉参考阈值",
                "default": 0.05
            },
            "enable_duration_limit": {
                "description": "启用音频时长限制",
                "type": "bool",
                "hint": "是否启用音频时长限制，超过1分钟的音频需要使用-q或-c参数",
                "default": true
            },
            "max_duration_limit": {
                "description": "最大音频时长限制(秒)",
                "type": "int",
                "hint": "音频的最大时长限制，超过此时长需要特殊处理",
                "default": 60
            }
        }
    },
    "cache_config": {
        "description": "缓存设置",
        "type": "object",
        "hint": "缓存相关的配置参数",
        "items": {
            "cache_dir": {
                "description": "缓存目录",
                "type": "string",
                "hint": "存储缓存文件的目录路径",
                "default": "data/cache/so-vits-svc"
            },
            "max_cache_size": {
                "description": "最大缓存大小(字节)",
                "type": "int",
                "hint": "缓存的最大容量，默认1GB",
                "default": 1073741824
            },
            "max_cache_age": {
                "description": "最大缓存时间(秒)",
                "type": "int",
                "hint": "缓存文件的最大保存时间，默认7天",
                "default": 604800
            }
        }
    },
    "volc_chorus": {
        "description": "火山副歌检测API配置",
        "type": "object",
        "hint": "用于副歌检测的火山API密钥配置",
        "items": {
            "ak": {
                "description": "火山AK",
                "type": "string",
                "default": ""
            },
            "sk": {
                "description": "火山SK",
                "type": "string",
                "default": ""
            },
            "appkey": {
                "description": "火山AppKey",
                "type": "string",
                "default": ""
            }
        }
    },
    "command_config": {
        "description": "命令设置",
        "type": "object",
        "hint": "命令相关的配置参数",
        "items": {
            "convert_command_aliases": {
                "description": "转换命令别名",
                "type": "list",
                "hint": "转换命令的别名列表，可以自定义多个别名。例如：[\"唱歌\", \"变声\", \"转换\"]",
                "default": ["牢剑唱", "转换"]
            }
        }
    }
} 