{
    "base_setting": {
        "description": "基础设置",
        "type": "object",
        "hint": "API服务器的基本配置",
        "items": {
            "base_url": {
                "description": "API服务器地址",
                "type": "string",
                "hint": "如果是本地部署，可以使用 http://127.0.0.1:1145",
                "default": "http://localhost:1145"
            },
            "timeout": {
                "description": "请求超时时间(秒)",
                "type": "int",
                "hint": "转换请求的超时时间",
                "default": 300
            },
            "msst_url": {
                "description": "MSST-WebUI API地址",
                "type": "string",
                "hint": "MSST-WebUI 的 API 地址",
                "default": "http://localhost:9000"
            },
            "msst_preset": {
                "description": "MSST预设文件路径",
                "type": "string",
                "hint": "MSST 处理使用的预设文件路径",
                "default": "wav.json"
            },
            "netease_cookie": {
                "description": "网易云音乐Cookie",
                "type": "string",
                "hint": "用于访问网易云音乐API的Cookie",
                "default": "MUSIC_;os=pc;appver=8.9.75;"
            }
        }
    },
    "voice_config": {
        "description": "语音转换设置",
        "type": "object",
        "hint": "语音转换的相关参数设置",
        "items": {
            "max_queue_size": {
                "description": "最大队列大小",
                "type": "int",
                "hint": "超过此队列大小将拒绝新的转换请求",
                "default": 100
            },
            "default_speaker": {
                "description": "默认说话人ID",
                "type": "string",
                "hint": "默认使用的说话人ID",
                "default": "0"
            },
            "default_pitch": {
                "description": "默认音调调整",
                "type": "int",
                "hint": "默认的音调调整值，范围-12到12",
                "default": 0
            }
        }
    },
    "command_config": {
        "description": "指令配置",
        "type": "object",
        "hint": "自定义指令设置",
        "items": {
            "convert_voice": {
                "description": "转换语音指令",
                "type": "string",
                "hint": "用于转换语音的指令，默认为 convert_voice",
                "default": "convert_voice"
            },
            "svc_status": {
                "description": "服务状态指令",
                "type": "string",
                "hint": "用于查看服务状态的指令，默认为 svc_status",
                "default": "svc_status"
            },
            "svc_presets": {
                "description": "预设列表指令",
                "type": "string",
                "hint": "用于查看预设列表的指令，默认为 svc_presets",
                "default": "svc_presets"
            },
            "svc_speakers": {
                "description": "说话人列表指令",
                "type": "string",
                "hint": "用于查看说话人列表的指令，默认为 svc_speakers",
                "default": "svc_speakers"
            },
            "cancel_convert": {
                "description": "取消转换指令",
                "type": "string",
                "hint": "用于取消转换任务的指令，默认为 cancel_convert",
                "default": "cancel_convert"
            }
        }
    }
} 